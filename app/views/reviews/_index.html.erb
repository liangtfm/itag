<% restaurant.reviews.each do |review| %>

<strong>"<%= review.title %>"</strong> by <%= link_to review.user.username, user_url(review.user) %>
<br>
Rating: <%= review.rating %>
<br>
<%= review.body %>
  <% if current_user && current_user.admin == true %>
    <%= button_to "Delete Review", review_url(review), method: :delete %>
  <% end %>
<br><br>
<% end %>