<h3><%= user.username %> follows these reviewers!</h3>
<ul>
<% user.followed_users.each do |user| %>
  <li><%= link_to user.username, user_url(user) %></li>
<% end %>
</ul>

<h3>Reviewers who follow <%= user.username %>!</h3>
<ul>
<% user.followers.each do |follower| %>
  <li><%= link_to follower.username, user_url(follower) %></li>
<% end %>
</ul>
