<h1>Welcome to <%= @user.username %>'s Profile!</h1>
<% if current_user == @user %>
  <%= link_to "Edit Profile", edit_user_url(@user) %>
<% end %>

<ul>
  <% if @user.location %>
    <li> <%= @user.location %> </li>
  <% end %>

  <% if @user.age %>
    <li> <%= @user.age %> </li>
  <% end %>

  <% if @user.gender %>
    <li> <%= @user.gender %> </li>
  <% end %>
</ul>



Biography:
<p><%= @user.biography %></p>


<h3>Reviews:</h3>


<% @user.reviews.each do |review| %>

<strong><%= link_to review.restaurant.name, restaurant_url(review.restaurant) %></strong>
  <% if current_user == @user %>
    (<%= link_to "Edit Review", edit_review_url(review) %>)
  <% end %>
<br>
<%= review.rating %>
<br>
<%= review.title %>
<br>
<%= review.body %>
<br><br>
<% end %>