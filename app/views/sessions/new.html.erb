<%= link_to "Back", cities_url %>


<form class="sign-in" action="<%= session_url %>" method="post">

  <h1>Welcome!</h1>

  <input
     name="authenticity_token"
     type="hidden"
     value="<%= form_authenticity_token %>">

  <div class="input">
  <label for="user_username">Username</label>
  <input
     id="user_username"
     name="user[username]"
     type="text">
  <br>

  <label for="user_password">Password</label>
  <input
     id="user_password"
     name="user[password]"
     type="password">
  <br>
  </div>

  <div class="submit">
  <input type="submit" value="Log in">
  </div>
</form>